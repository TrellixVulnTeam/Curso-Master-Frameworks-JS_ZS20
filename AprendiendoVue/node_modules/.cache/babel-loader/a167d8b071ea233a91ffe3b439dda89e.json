ACA,YAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,SAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA;AACA,OAJA;AAKA,KAPA;AAQA,IAAA,aARA,yBAQA,SARA,EAQA;AAAA;;AACA,MAAA,IAAA,CAAA;AACA,QAAA,KAAA,EAAA,4CADA;AAEA,QAAA,IAAA,EAAA,iDAFA;AAGA,QAAA,IAAA,EAAA,SAHA;AAIA,QAAA,OAAA,EAAA,IAJA;AAKA,QAAA,UAAA,EAAA;AALA,OAAA,CAAA,CAMA,IANA,CAMA,UAAA,UAAA,EAAA;AACA,YAAA,UAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,GAAA,GAAA,UAAA,GAAA,SAAA,EAAA,IAAA,CAAA,YAAA;AACA,YAAA,IAAA,CACA,kBADA,EAEA,yCAFA,EAGA,SAHA,CAAA;;AAKA,YAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA,WAPA;AAQA,SATA,MASA;AACA,UAAA,IAAA,CAAA,0BAAA,CAAA;AACA;AACA,OAnBA;AAoBA;AA7BA;AAfA,CAAA","sourcesContent":["<template>\r\n  <div class=\"general\">\r\n    <div class=\"center\">\r\n      <section id=\"content\">\r\n        <article class=\"article-item article-detail\" v-if=\"article\">\r\n          <div class=\"image-wrap\">\r\n            <img\r\n              :src=\"url + 'get-image/' + article.image\"\r\n              :alt=\"article.title\"\r\n              v-if=\"article.image\"\r\n            />\r\n            <img\r\n              src=\"https://png.pngtree.com/png-vector/20190223/ourmid/pngtree-vector-picture-icon-png-image_695350.jpg\"\r\n              :alt=\"article.title\"\r\n              v-else\r\n            />\r\n          </div>\r\n\r\n          <h1 class=\"subheader\">{{ article.title }}</h1>\r\n\r\n          <span class=\"date\"> {{ article.date | moment(\"from\", \"now\") }} </span>\r\n          <p>{{ article.content }}</p>\r\n          <div class=\"clearfix\"></div>\r\n\r\n          <router-link :to=\"'/editar/' + article._id\" class=\"btn btn-warning\"\r\n            >Editar</router-link\r\n          >\r\n          <a @click=\"deleteArticle(article._id)\" class=\"btn btn-danger\"\r\n            >Eliminar</a\r\n          >\r\n        </article>\r\n      </section>\r\n      <Sidebar></Sidebar>\r\n      <div class=\"clearfix\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport Sidebar from \"./Sidebar.vue\";\r\nimport Global from \"../Global\";\r\nimport swal from \"sweetalert\";\r\n\r\nexport default {\r\n  name: \"Article\",\r\n  components: {\r\n    Sidebar,\r\n  },\r\n  data() {\r\n    return {\r\n      url: Global.url,\r\n      article: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    var articleId = this.$route.params.id;\r\n    this.getArticle(articleId);\r\n  },\r\n  methods: {\r\n    getArticle(articleId) {\r\n      axios.get(this.url + \"article/\" + articleId).then((res) => {\r\n        if (res.data.status === \"success\") {\r\n          this.article = res.data.article;\r\n        }\r\n      });\r\n    },\r\n    deleteArticle(articleId) {\r\n      swal({\r\n        title: \"Estas seguro de querer borrar el artículo?\",\r\n        text: \"Si lo eliminas, no podras recuperar el artículo\",\r\n        icon: \"warning\",\r\n        buttons: true,\r\n        dangerMode: true,\r\n      }).then((willDelete) => {\r\n        if (willDelete) {\r\n          axios.delete(this.url + \"article/\" + articleId).then(() => {\r\n            swal(\r\n              \"Artículo borrado\",\r\n              \"El artículo se ha borrado correctamente\",\r\n              \"success\"\r\n            );\r\n            this.$router.push(\"/blog\");\r\n          });\r\n        } else {\r\n          swal(\"Tu artículo esta a salvo\");\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}