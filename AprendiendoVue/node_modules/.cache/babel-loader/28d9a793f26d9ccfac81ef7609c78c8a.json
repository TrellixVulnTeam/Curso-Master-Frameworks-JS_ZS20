ACA,QAAA,QAAA,EAAA,QADA;AAEA,QAAA,SAAA,EAAA,SAAA,CAAA,CAAA;AAFA,OALA;AASA,MAAA,GAAA,EAAA;AACA,QAAA,QAAA,EAAA,QADA;AAEA,QAAA,SAAA,EAAA,SAAA,CAAA,EAAA;AAFA;AATA;AADA,GAhBA;AAgCA,EAAA,OAAA,EAAA;AACA,IAAA,cADA,4BACA;AACA,WAAA,SAAA,GAAA,IAAA,CADA,CAGA;;AACA,WAAA,EAAA,CAAA,MAAA;;AACA,UAAA,KAAA,EAAA,CAAA,QAAA,EAAA;AACA,eAAA,KAAA;AACA;;AAEA,MAAA,KAAA,CAAA,mBAAA,CAAA;AACA;AAXA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"general\">\r\n    <div class=\"center\">\r\n      <section id=\"content\">\r\n        <h2 class=\"subheader\">Formulario</h2>\r\n        <form class=\"mid-form\" @submit.prevent=\"mostrarUsuario()\">\r\n          <div class=\"form-group\">\r\n            <label for=\"nombre\">Nombre</label>\r\n            <input type=\"text\" name=\"nombre\" v-model=\"user.nombre\" />\r\n            <div v-if=\"submitted && !$v.user.nombre.required\">Este campo es requerido</div>\r\n            <div v-if=\"submitted && !$v.user.nombre.minLength\">\r\n              Este campo debe tener mas caracteres\r\n            </div>\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"apellidos\">Apellidos</label>\r\n            <input type=\"text\" name=\"apellidos\" v-model=\"user.apellidos\" />\r\n            <div v-if=\"submitted && !$v.user.apellidos.required\">\r\n              Este campo es requerido\r\n            </div>\r\n            <div v-if=\"submitted && !$v.user.apellidos.minLength\">\r\n              Este campo debe tener mas caracteres\r\n            </div>\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"bio\">Biografia</label>\r\n            <textarea name=\"bio\" v-model=\"user.bio\"></textarea>\r\n            <div v-if=\"submitted && !$v.user.bio.required\">Este campo es requerido</div>\r\n            <div v-if=\"submitted && !$v.user.bio.minLength\">\r\n              Este campo debe tener mas caracteres\r\n            </div>\r\n          </div>\r\n          <div class=\"form-group radiobuttons\">\r\n            <input\r\n              type=\"radio\"\r\n              name=\"genero\"\r\n              value=\"Hombre\"\r\n              checked\r\n              v-model=\"user.genero\"\r\n            />\r\n            Hombre\r\n            <input\r\n              type=\"radio\"\r\n              name=\"genero\"\r\n              value=\"Mujer\"\r\n              v-model=\"user.genero\"\r\n            />\r\n            Mujer\r\n            <input\r\n              type=\"radio\"\r\n              name=\"genero\"\r\n              value=\"Otro\"\r\n              v-model=\"user.genero\"\r\n            />\r\n            Otro\r\n          </div>\r\n\r\n          <div class=\"clearfix\"></div>\r\n\r\n          <input type=\"submit\" value=\"Enviar\" class=\"btn btn-success\" />\r\n        </form>\r\n\r\n        <div class=\"datos\" v-if=\"user.nombre && user.apellidos\">\r\n          <h3>{{ user.nombre + \" \" + user.apellidos }}</h3>\r\n        </div>\r\n      </section>\r\n      <Sidebar></Sidebar>\r\n      <div class=\"clearfix\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Sidebar from \"./Sidebar.vue\";\r\n// Vuelidate (Libreria para validacion de campos)\r\nimport { required, minLength } from \"vuelidate/lib/validators\";\r\n\r\nexport default {\r\n  name: \"Formulario\",\r\n  components: {\r\n    Sidebar,\r\n  },\r\n  data() {\r\n    return {\r\n      submitted: false,\r\n      user: {\r\n        nombre: \"\",\r\n        apellidos: \"\",\r\n        bio: \"\",\r\n        genero: \"\",\r\n      },\r\n    };\r\n  },\r\n  validations: {\r\n    user: {\r\n      nombre: {\r\n        required,\r\n        minLength: minLength(2),\r\n      },\r\n      apellidos: {\r\n        required,\r\n        minLength: minLength(2),\r\n      },\r\n      bio: {\r\n        required,\r\n        minLength: minLength(10),\r\n      },\r\n    },\r\n  },\r\n  methods: {\r\n    mostrarUsuario() {\r\n      this.submitted = true;\r\n\r\n      // $touch verifica si fueron tocados los campos a validar\r\n      this.$v.$touch();\r\n      if (this.$v.$invalid) {\r\n        return false;\r\n      }\r\n\r\n      alert(\"Validacion pasada\");\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}